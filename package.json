{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon server.js --exec babel-node",
    "lint": "eslint . --fix",
    "test-migrate": "cross-env NODE_ENV=test sequelize db:migrate",
    "undo-test-migrate": "cross-env NODE_ENV=test sequelize db:migrate:undo:all",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text --reporter=lcov mocha --timeout 250000 tests/* --require babel-register --exit",
    "cover": "NODE_ENV=test istanbul cover _mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && npm run build-babel",
    "build-babel": "babel . --ignore node_modules,test,migrations -d ./dist -s --copy-files",
    "postinstall": "npm run build && ./node_modules/.bin/sequelize db:migrate"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.3.0",
    "method-override": "^2.3.10",
    "methods": "^1.1.2",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-facebook-token": "^3.3.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-google-plus-token": "^2.1.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "passport-twitter-token": "^1.3.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "request": "^2.87.0",
    "sequelize": "^4.39.0",
    "sequelize-cli": "^4.1.1",
    "slug": "^0.9.1",
    "swagger-ui-express": "^4.0.1",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.4",
    "nyc": "^13.0.1"
  }
}
